ArBA;AAsBA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,KADA,EACA;AACA,WAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,MAJA,oBAIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,QAFA,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,aAAA,EAAA,MAAA,CAAA,aAFA;AAGA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAHA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AAUA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAlBA;AAmBA,IAAA,QAnBA,sBAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA3BA;AA4BA,IAAA,QA5BA,oBA4BA,IA5BA,EA4BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAKA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AANA,kDAOA,IAPA;;AAAA;AAAA;AAAA;AASA,gBAAA,OAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAvCA;AAwCA,IAAA,UAxCA,sBAwCA,IAxCA,EAwCA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KA3CA;AA4CA,IAAA,UA5CA,sBA4CA,IA5CA,EA4CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA;AArDA;AAtBA,CAAA","sourcesContent":["\r\n<template>\r\n<div class=\"edit-pokemon\">\r\n\r\n<h1>Edit Pokemon Page</h1>\r\n<div class=\"heading\">\r\n  <div class=\"circle\">1</div>\r\n  <h2>Add a Pokemon</h2>\r\n</div>\r\n<div class=\"add\">\r\n  <div class=\"form\">\r\n    <input v-model=\"title\" placeholder=\"Pokemon Name\">\r\n    <hr>\r\n    <input v-model=\"pokedexNumber\" placeholder=\"Pokedex Number\">\r\n    <p></p>\r\n    <hr>\r\n    <h5>Add a photo of the Pokemon here!</h5>\r\n    <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\r\n    <button @click=\"upload\">Add to Pokedex</button>\r\n  </div>\r\n  <div class=\"upload\" v-if=\"addItem\">\r\n    <h2>{{addItem.title}}</h2>\r\n    <h2>{{addItem.pokedexNumber}}</h2>\r\n    <img :src=\"addItem.path\" />\r\n  </div>\r\n</div>\r\n\r\n<hr>\r\n\r\n<div class=\"heading\">\r\n  <div class=\"circle\">2</div>\r\n  <h2>Search for/Delete a Pokemon</h2>\r\n</div>\r\n<div class=\"edit\">\r\n  <div class=\"form\">\r\n    <input v-model=\"findTitle\" placeholder=\"Search\">\r\n    <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\r\n      <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"upload\" v-if=\"findItem\">\r\n    <input v-model=\"findItem.title\">\r\n    <p></p>\r\n    <img :src=\"findItem.path\" />\r\n  </div>\r\n  <div class=\"actions\" v-if=\"findItem\">\r\n    <button @click=\"deleteItem(findItem)\">Delete</button>\r\n  </div>\r\n</div>\r\n\r\n\r\n</div>\r\n</template>\r\n\r\n<style scoped>\r\n.image h2 {\r\n  font-style: italic;\r\n  font-size: 1em;\r\n}\r\n\r\n.heading {\r\n  display: flex;\r\n  margin-bottom: 20px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.heading h2 {\r\n  margin-top: 8px;\r\n  margin-left: 10px;\r\n}\r\n\r\n.add,\r\n.edit {\r\n  display: flex;\r\n}\r\n\r\n.circle {\r\n  border-radius: 50%;\r\n  width: 18px;\r\n  height: 18px;\r\n  padding: 8px;\r\n  background: #333;\r\n  color: #fff;\r\n  text-align: center\r\n}\r\n\r\n/* Form */\r\ninput,\r\ntextarea,\r\nselect,\r\nbutton {\r\n  font-family: 'Montserrat', sans-serif;\r\n  font-size: 1em;\r\n}\r\n\r\n.form {\r\n  margin-right: 50px;\r\n}\r\n\r\n/* Uploaded images */\r\n.upload h2 {\r\n  margin: 0px;\r\n}\r\n\r\n.upload img {\r\n  max-width: 300px;\r\n}\r\n\r\n/* Suggestions */\r\n.suggestions {\r\n  width: 200px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.suggestion {\r\n  min-height: 20px;\r\n}\r\n\r\n.suggestion:hover {\r\n  background-color: #5BDEFF;\r\n  color: #fff;\r\n}\r\n\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Admin',\r\n  data() {\r\n  return {\r\n  findTitle: \"\",\r\n  findItem: null,\r\n  items: [],\r\n    title: \"\",\r\n    pokedexNumber: \"\",\r\n    file: null,\r\n    addItem: null,\r\n  }\r\n  },\r\n  computed: {\r\n  suggestions() {\r\n    let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\r\n    return items.sort((a, b) => a.title > b.title);\r\n  }\r\n  },\r\n  created() {\r\n  this.getItems();\r\n  },\r\n  methods: {\r\n  fileChanged(event) {\r\n    this.file = event.target.files[0]\r\n  },\r\n  async upload() {\r\n  try {\r\n    const formData = new FormData();\r\n    formData.append('photo', this.file, this.file.name)\r\n    let r1 = await axios.post('/api/photos', formData);\r\n    let r2 = await axios.post('/api/items', {\r\n      title: this.title,\r\n      pokedexNumber: this.pokedexNumber,\r\n      path: r1.data.path\r\n    });\r\n    this.addItem = r2.data;\r\n  } catch (error) {\r\n    console.log(error);\r\n  }\r\n  },\r\n  async getItems() {\r\n  try {\r\n    let response = await axios.get(\"/api/items\");\r\n    this.items = response.data;\r\n    return true;\r\n  } catch (error) {\r\n    console.log(error);\r\n  }\r\n},\r\nasync editItem(item) {\r\n  try {\r\n    await axios.put(\"/api/items/\" + item._id, {\r\n      title: this.findItem.title,\r\n    });\r\n    this.findItem = null;\r\n    this.getItems();\r\n    return true;\r\n  } catch (error) {\r\n    console.log(error);\r\n  }\r\n},\r\nselectItem(item) {\r\n  this.findTitle = \"\";\r\n  this.findItem = item;\r\n},\r\nasync deleteItem(item) {\r\n  try {\r\n    await axios.delete(\"/api/items/\" + item._id);\r\n    this.findItem = null;\r\n    this.getItems();\r\n    return true;\r\n  } catch (error) {\r\n    console.log(error);\r\n  }\r\n}\r\n}\r\n\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views"}]}